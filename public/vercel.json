{
  "redirects": [
    
    { "source": "/store", "destination": "/gallery.html" },
    { "source": "/produkt/:slug*", "destination": "/gallery.html" },
    { "source": "/produkt-kategoriya/:slug*", "destination": "/gallery.html" },

    
    { "source": "/my-account", "destination": "/" },
    { "source": "/my-account/:path*", "destination": "/" },
    { "source": "/contact-us", "destination": "/locations.html" },  
    { "source": "/contact", "destination": "/locations.html" },

    
    { "source": "/index.php", "destination": "/" },
    { "source": "/home", "destination": "/" },

    
    { "source": "/:path*",
      "destination": "/gallery.html",
      "has": [
        { "type": "query", "key": "p" },
        { "type": "query", "key": "product_id" },
        { "type": "query", "key": "page_id" }
      ]
    },

    
    { "source": "/:path*", "destination": "/" }
  ],

  "cleanUrls": true,
  "trailingSlash": false
}
